{
  "plugins": ["stylelint-prettier"],
  "rules": {
    "prettier/prettier": true
  },
  "extends": [
    "stylelint-config-standard",
    "@efficientcss/stylelint-config-ecss"
  ],
  "overrides": [
    {
      "files": ["*"],
      "rules": {
        "plugin/selector-starts-with-filename": null,
        "rule-selector-property-disallowed-list": null
      }
    }
  ]
}
